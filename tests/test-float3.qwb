function void testfloat(float a, float b){
  printf(a + b);
  printf(a - b);
  printf(a * b);
  printf(a / b);
  printb(a == b);
  printb(a == a);
  printb(a != b);
  printb(a != a);
  printb(a > b);
  printb(a >= b);
  printb(a < b);
  printb(a <= b);
}

function int main(){
  float c;
  float d;

  c = 25.0;
  d = 5.12345;

  testfloat(c, d);
  testfloat(d, d);

}
